{
  "schemaVersion": 1,
  "id": "awscraftconnect",
  "version": "${version}",
  "name": "AWSCraftConnect",
  "description": "A simple client side mod that allows for easy connection to AWSCraft servers.",
  "authors": [
    "Ryan Steffan & Contributors"
  ],
  "contact": {
    "homepage": "https://github.com/ryansteffan/awscraftconnect"
  },
  "license": "AGPL-3.0",
  "icon": "assets/awscraftconnect/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.ryansteffan.awscraftconnect.fabric.AwscraftconnectFabric"
    ],
    "client": [
      "com.ryansteffan.awscraftconnect.fabric.client.AwscraftconnectFabricClient"
    ]
  },
  "mixins": [
    "awscraftconnect.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.18.4",
    "minecraft": "~1.20.1",
    "java": ">=17",
    "architectury": ">=9.2.14",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
}
